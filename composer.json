{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.19",
        "barryvdh/laravel-cors": "^0.11.1",
        "braintree/braintree_php": "^4.8",
        "consoletvs/charts": "5.*",
        "cyrildewit/eloquent-viewable": "^3.0",
        "devmarketer/easynav": "^1.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.8",
        "intervention/image": "^2.4",
        "kevupton/laravel-coinpayments": "^1.1",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.5",
        "laravel/socialite": "4.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "laravolt/avatar": "^3.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "paypal/rest-api-sdk-php": "*",
        "pbmedia/laravel-ffmpeg": "3.0.2",
        "pragmarx/tracker": "^3.2",
        "razorpay/razorpay": "^2.5",
        "socialiteproviders/gitlab": "^3.1.0",
        "spatie/async": "^1.1",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.31",
        "spatie/laravel-translatable": "^2.2",
        "stripe/stripe-php": "^5.8",
        "supervisorphp/supervisor": "^3.0",
        "tzsk/payu": "^2.0",
        "unicodeveloper/laravel-paystack": "^1.0",
        "vimeo/laravel": "^5.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
